.img{
    height: 120rpx;
     width: 100%;
     display: flex;
     align-items: center;
     justify-content: center;
}
.word_list{
   width: 100%;
   height: 80rpx;
   flex-direction: column;
   background-color: #2ba99b;
   border-color: balck;
   color:white;
   position:center;
   text-underline-position: center;
   float:center;
   background-position:center;
   align-content: flex-start;
   background-repeat: round;
   display: flex;
   align-items: center;
   justify-content: center;
}
.book_list{
   width: 100%;
   height: 120rpx;
   flex-direction: column;
   background-color: #2ba99b;
   border-color: balck;
   color:white;
   position:center;
   font-size: 50rpx;
   font-weight: 800;
   text-underline-position: center;
   float:center;
   background-position:center;
   align-content: flex-start;
   background-repeat: round;
   display: flex;
   align-items: center;
   justify-content: center;
}